{
  "name": "my_react_start_kit",
  "version": "1.0.0",
  "description": "simple react start kit for development",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --config=jest.config.json --coverage",
    "test:f": "jest --config=jest.config.json",
    "dev": "webpack-dev-server --mode development --open --hot --config webpack.config.js",
    "lint": "eslint ./src",
    "clean": "rimraf dist/*",
    "dist": "npm run clean && webpack --progress --bail --env dist -p",
    "deploy": "npm run dist && gh-pages -d dist",
    "start": "node server",
    "heroku-postbuild": "npm install && npm run dist",
    "build": "npm run clean && webpack --mode production --config webpack.config.js"
  },
  "author": "Jason Chang",
  "license": "ISC",
  "dependencies": {
    "@antv/data-set": "^0.10.1",
    "@babel/runtime": "^7.5.5",
    "antd": "^3.9.3",
    "axios": "^0.19.0",
    "bizcharts": "^3.5.8",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-ga": "^2.7.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",

    "jsdom-global": "^3.0.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "react-hot-loader": "^4.3.8",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
